import java.util.Scanner;
public class Q1
{
  public static void main(String[]args)
  {
    Scanner sc=new Scanner(System.in);
   for(int i=1;i<=10;i++)
    {
      int digit=0;
      System.out.println("please enter a number");
    int n=sc.nextInt();
   while(n>0)
    {
      if(n>100)
      {
        digit=n%100;
        System.out.println(digit);
    n=n/10;
      }
      else
      {
        if(n>10 && n<100)
        {
          digit=n%10;
          System.out.println(digit);
    n=n/10;
        }
        else
        {
          System.out.println(n);
    
        }
      }
       
    }
    
    }
  }
}